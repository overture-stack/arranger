  },
  "homepage": "https://github.com/overture-stack/arranger#readme",
  "dependencies": {
    "@arranger/middleware": "^2.19.4",
    "@babel/polyfill": "^7.12.1",
    "graphql-fields": "^2.0.3",
    "kind-of": "^6.0.3",
    "lodash": "^4.17.20",
    "minimist": "^1.2.5",
    "node-fetch": "^2.2.0",
    "uuid": "^3.2.1"
  },
  "peerDependencies": {
    "graphql": "^14.5.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",

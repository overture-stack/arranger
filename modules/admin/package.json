{
  "name": "@arranger/admin",
  "version": "1.1.0",
  "description": "Admin API for the Arranger server in graphql",
  "engineStrict": true,
  "engines": {
    "node": ">= 8.9.0"
  },
  "devDependencies": {
    "@types/express": "^4.16.0",
    "@types/graphql": "^14.0.0",
    "@types/jest": "~23.3.1",
    "@types/jwt-decode": "^2.2.1",
    "@types/node": "~8.10.25",
    "@types/ramda": "^0.25.42",
    "jest": "~23.5.0",
    "nodemon": "^1.18.4",
    "npm-watch": "^0.4.0",
    "prettier": "~1.14.2",
    "rimraf": "~2.6.2",
    "ts-jest": "~23.1.3",
    "tslint": "~5.11.0",
    "tslint-config-prettier": "1.14.0",
    "tslint-microsoft-contrib": "~5.2.0",
    "tsutils": "~3.0.0",
    "typescript": "~3.0.1"
  },
  "watch": {
    "lint": {
      "patterns": [
        "src"
      ],
      "extensions": "ts",
      "quiet": false
    }
  },
  "scripts": {
    "clean": "rimraf coverage build tmp",
    "build": "tsc -p tsconfig.release.json",
    "watch": "rm -rf dist && tsc -w -p tsconfig.release.json",
    "lint": "tslint -t stylish --project \"tsconfig.json\"",
    "watch:lint": "npm-watch lint",
    "pretest": "npm run lint",
    "test": "npm run test-only",
    "test-only": "jest --coverage",
    "test:watch": "jest --watch",
    "watch:run": "nodemon ./build/src",
    "prepare": "npm run build"
  },
  "author": "Minh Ha",
  "license": "APLv2",
  "dependencies": {
    "@arranger/mapping-utils": "^1.1.0",
    "@arranger/schema": "^1.1.0",
    "@types/elasticsearch": "^5.0.26",
    "apollo-link-http": "^1.5.5",
    "apollo-server": "^2.1.0",
    "apollo-server-express": "^2.1.0",
    "convert-units": "^2.3.4",
    "date-fns": "^1.29.0",
    "elasticsearch": "^15.1.1",
    "express": "^4.16.3",
    "graphql": "^14.0.2",
    "graphql-tools": "^4.0.0",
    "graphql-type-json": "^0.2.1",
    "jwt-decode": "^2.2.0",
    "node-fetch": "^2.2.0",
    "qew": "^0.9.13",
    "ramda": "^0.26.1",
    "tslib": "~1.9.3",
    "typegql": "^0.6.0"
  }
}

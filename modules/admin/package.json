{
  "name": "@arranger/admin",
  "version": "2.6.1",
  "description": "Admin API for the Arranger server in graphql",
  "engineStrict": true,
  "engines": {
    "node": ">= 8.9.0"
  },
  "devDependencies": {
    "husky": "^4.3.0",
    "nodemon": "^1.18.4",
    "npm-watch": "^0.4.0",
    "prettier": "^2.1.2",
    "pretty-quick": "^3.0.2",
    "rimraf": "~2.6.2",
    "ts-jest": "~23.1.3",
    "tslint": "~5.11.0",
    "tslint-config-prettier": "1.14.0",
    "tslint-microsoft-contrib": "~5.2.0",
    "tsutils": "~3.0.0"
  },
  "watch": {
    "lint": {
      "patterns": [
        "src"
      ],
      "extensions": "ts",
      "quiet": false
    }
  },
  "scripts": {
    "clean": "rimraf coverage build tmp",
    "build": "tsc -p tsconfig.release.json",
    "watch": "rm -rf dist && tsc -w -p tsconfig.release.json",
    "lint": "tslint -t stylish --project \"tsconfig.json\"",
    "watch:lint": "npm-watch lint",
    "pretest": "npm run lint",
    "test": "npm run test-only",
    "test-only": "jest --coverage",
    "test:watch": "jest --watch",
    "watch:run": "nodemon ./build/src",
    "prepare": "npm run build"
  },
  "author": "Minh Ha",
  "license": "APLv2",
  "dependencies": {
    "@arranger/mapping-utils": "^2.6.1",
    "@arranger/schema": "^2.6.1",
    "@elastic/elasticsearch": "7.5.0",
    "apollo-link-http": "^1.5.5",
    "apollo-server": "^2.14.2",
    "apollo-server-express": "^2.14.2",
    "convert-units": "^2.3.4",
    "date-fns": "^1.29.0",
    "express": "^4.16.3",
    "graphql": "^14.5.3",
    "graphql-tools": "^4.0.0",
    "graphql-type-json": "^0.2.1",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.20",
    "node-fetch": "^2.2.0",
    "qew": "^0.9.13",
    "ramda": "^0.26.1",
    "tslib": "^1.10.0"
  },
  "prettier": {
    "printWidth": 100,
    "trailingComma": "all",
    "singleQuote": true
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}

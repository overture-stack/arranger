		"typescript-transform-paths": "^3.5.3"
	},
	"imports": {
		"#public": "./public",
		"#*": "./src/*"
	},
	"peerDependencies": {
		"@emotion/react": "^11.0.0",
		"@emotion/styled": "^11.0.0",
		"@types/react": "^17.0.0 || ^18.0.0",
		"@types/react-dom": "^17.0.0 || ^18.0.0",
		"react": "^17.0.0 || ^18.0.0",
		"react-dom": "^17.0.0 || ^18.0.0"
	},
	"overrides": {
		"minimist": "^1.2.6"
	},
	"type": "module"
}

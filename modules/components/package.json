{
	"name": "@overture-stack/arranger-components",
	"description": "Data Portal Components",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"build": "babel src --copy-files --extensions .tsx,.ts,.js,.jsx --out-dir dist",
		"build:production": "npm run patchTS && npm run build && npm run declarations -- --project tsconfig.release.json",
		"build-storybook": "build-storybook -s ./public",
		"cleanDist": "rm -rf dist",
		"dashboard": "STORYBOOK_DASHBOARD=true start-storybook -p 6060 -c .storybook -s ./public",
		"declarations": "tsc --emitDeclarationOnly",
		"ifPublish:declarations": "if test \"$PUBLISH_DECLARATIONS\" = \"true\" ; then (npm run -s declarations || true) ; fi",
		"lint": "eslint ./src --ext=.jsx,.js,.tsx,.ts",
		"patchTS": "ts-patch install -s",
		"portal": "STORYBOOK_PORTAL=true start-storybook -p 7070 -c .storybook -s ./public",
		"storybook": "start-storybook -p 8080 -c .storybook -s ./public",
		"test": "NODE_OPTIONS=\"$NODE_OPTIONS --experimental-vm-modules\" jest --config=jest.config.ts",
		"watch": "npm run cleanDist && (concurrently -n babel,tsc -c yellow,blue \"npm run build -- --watch\" \"npm run declarations -- --watch\")",
		"watchnew": "npm run cleanDist && (concurrently -n babel,tsc -c yellow,blue \"npm run build -- --watch\")"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/overture-stack/arranger.git"
	},
	"bugs": {
		"url": "https://github.com/overture-stack/arranger/issues"
	},
	"homepage": "https://github.com/overture-stack/arranger#readme",
	"dependencies": {
		"@emotion/react": "^11.0.0",
		"@emotion/styled": "^11.0.0",
		"@overture-stack/sqon-builder": "^1.1.0",
		"@reach/component-component": "^0.17.0",
		"@tanstack/react-table": "^8.7.9",
		"axios": "^1.7.9",
		"classnames": "^2.3.2",
		"color": "^4.2.3",
		"convert-units": "^2.3.4",
		"date-fns": "^2.29.3",
		"downshift": "^1.23.2",
		"filesize": "^10.0.6",
		"formik": "^0.11.11",
		"jsonpath-plus": "^10.3.0",
		"lodash-es": "^4.17.21",
		"pluralize": "^8.0.0",
		"ramda": "^0.28.0",
		"react-datepicker": "^4.10.0",
		"react-grid-layout": "^0.16.5",
		"react-icons": "^5.4.0",
		"react-input-range": "^1.3.0",
		"react-router-dom": "^4.2.2",
		"react-scrollbar-size": "^2.1.0",
		"react-spinkit": "^3.0.0",
		"react-table-old": "npm:react-table@^6.11.5",
		"react-tippy": "^1.4.0",
		"react-toastify": "^3.3.2",
		"react-treeview": "^0.4.7",
		"recompose": "^0.30.0",
		"resolve-url": "^0.2.1",
		"rxjs": "^5.5.6",
		"semantic-ui-css": "^2.4.1",
		"semantic-ui-react": "^0.77.2",
		"sqon-builder": "^2.0.1",
		"url-join": "^4.0.1",
		"uuid": "^9.0.0"
	},
	"devDependencies": {
		"@babel/cli": "^7.26.4",
		"@babel/preset-env": "^7.26.9",
		"@babel/preset-react": "^7.26.3",
		"@babel/preset-typescript": "^7.26.0",
		"@emotion/babel-plugin": "^11.10.6",
		"@eslint/compat": "^1.2.5",
		"@storybook/addon-actions": "^3.3.8",
		"@storybook/addon-options": "^3.3.11",
		"@storybook/react": "^3.3.3",
		"@types/color": "^3.0.3",
		"@types/jest": "^29.5.0",
		"@types/lodash-es": "^4.17.12",
		"@types/pluralize": "^0.0.29",
		"@types/react-spinkit": "^3.0.7",
		"@types/url-join": "^4.0.1",
		"babel-plugin-module-resolver": "^5.0.0",
		"concurrently": "^9.1.2",
		"eslint-plugin-jsx-a11y": "^6.10.2",
		"eslint-plugin-react": "^7.37.4",
		"eslint-plugin-react-hooks": "^5.2.0",
		"jest": "^29.7.0",
		"prettier": "^3.4.2",
		"storybook-router": "^0.3.3",
		"ts-jest": "^29.0.5",
		"ts-node": "^10.9.2",
